{
  "name": "natours",
  "version": "1.0.0",
  "description": "exercise from udemy",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/styles.scss css/styles.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/styles.scss css/styles.compiled.css",
    "concat:css": "concat -o css/styles.concat.css css/icon-font.css css/styles.compiled.css",
    "compress:css": "node-sass css/styles.compiled.css css/styles.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css compress:css"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concat": "^1.0.3",
    "live-server": "^1.2.2",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5"
  }
}
